@using System.IO
@model List<FileInfo>

@{
    ViewBag.Title = "Video";
}

<h2>Video</h2>
    <video id="sampleVideo" class="video-js vjs-default-skin" controls preload="auto" width="640" height="264"
           poster="http://video-js.zencoder.com/oceans-clip.png" data-setup='{"example_option":true}'>
        <source src="@Url.Content("~/Content/videos/oceans.mp4")" type='video/mp4' />
        <source src="http://video-js.zencoder.com/oceans-clip.mp4" type='video/mp4' />
        <source src="http://video-js.zencoder.com/oceans-clip.webm" type='video/webm' />
        <source src="http://video-js.zencoder.com/oceans-clip.ogv" type='video/ogg' />
    </video>


